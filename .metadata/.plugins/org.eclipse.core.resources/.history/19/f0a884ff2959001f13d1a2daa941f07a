package ex;

public class Sample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double f1 = 20240813.0;
		double f2 = 20240814.0;
		double f3 = 20240816.0;
		
		System.out.printf("%f\n", f1);
		System.out.printf("%f\n", f2);
		System.out.printf("%f\n", f3);
		
		float f4 = 20240830.0f;
		float f5 = 20240840.0f;
		float f6 = 20240850.0f;
		
		System.out.printf("%f\n", f4);
		System.out.printf("%f\n", f5);
		System.out.printf("%f\n", f6);
		
	}

}
